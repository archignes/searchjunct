{
  "name": "Chatnoir",
  "id": "chatnoir",
  "search_link": "https://www.chatnoir.eu/?q=%s",
  "special_note": "academic research project",
  "indices": [{"name":"ClueWeb", "url":"https://lemurproject.org/clueweb12.php/"}, {"name":"CommonCrawl", "url":"https://commoncrawl.org/"}],
  "characteristics": {
    "output": "list of links"
  },
  "citations": [
    {
            "names": "Bevendorff, J., Stein, B., Hagen, M., Potthast, M.",
            "year": "2018",
            "title": "Elastic ChatNoir: Search Engine for the ClueWeb and the Common Crawl",
            "publication": "Advances in Information Retrieval",
            "doi": "10.1007/978-3-319-76941-7_83",
            "title_url": "https://downloads.webis.de/publications/papers/bevendorff_2018.pdf",
            "doi_url": "https://doi.org/10.1007/978-3-319-76941-7_83",
            "abstract": "Elastic ChatNoir is an Elasticsearch-based search engine offering a freely accessible search interface for the two ClueWeb corpora and the CommonCrawl, together about 3 billion web pages. Running across 130 nodes, Elastic ChatNoir features subsecond response times comparable to commercial search engines. Unlike most commercial search engines, it also offers a powerful API that is available free of charge to IR researchers. Elastic ChatNoir’s main purpose is to serve as a baseline for reproducible IR experiments and user studies for the coming years, empowering research at a scale not attainable to many labs beforehand, and to provide a platform for experimenting with new approaches to web search."
    }
  ],
  "twitter_link": "https://twitter.com/webis_de",
  "github_link": "https://github.com/chatnoir-eu",
  "youtube_link": "https://www.youtube.com/webis"
},

